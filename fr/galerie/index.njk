---
layout: base.njk
title: Galerie
lang: fr
permalink: /fr/galerie/
alt: {"es": "/galeria/", "en": "/en/gallery/"}
---
<section class="section container">
  <h1>Galerie</h1>
  <div class="gallery-grid">
    {% for img in gallery %}
      <figure class="g-item">
        <img src="{{ img.src }}" alt="{{ (img.alt and img.alt['fr']) or 'Photo du bar' }}">
      </figure>
    {% endfor %}
  </div>
</section>
